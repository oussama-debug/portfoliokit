{
  "name": "portfoliosixkit",
  "private": true,
  "scripts": {
    "build": "env-cmd -f ./packages/env/.env turbo run build",
    "dev": "env-cmd -f ./packages/env/.env turbo run dev",
    "lint": "env-cmd -f ./packages/env/.env turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "env-cmd -f ./packages/env/.env turbo run check-types",
    "setup": "env-cmd -f ./packages/env/.env turbo run setup"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "env-cmd": "^11.0.0"
  }
}
